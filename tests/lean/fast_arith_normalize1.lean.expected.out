>> x + x + x ==> 3 * x
>> x + y + x + y ==> 2 * x + 2 * y
>> x + y + x + y + x + y ==> 3 * x + 3 * y
>> x + y + x + y + 1 ==> 1 + (2 * x + 2 * y)
>> 1 + x + y + x + y + 1 ==> 2 + (2 * x + 2 * y)
>> 0 + 0 * x + x + y + -1 * x + y + 1 ==> 1 + 2 * y
>> 0 + 0 * x + x + y + -1 * x + -1 * y + 1 ==> 1
>> 0 + 0 * x + x + y + -1 * x + -1 * y + 1 + -1 ==> 0
>> 1 + 2 + 3 + x + 2 * x + 1 ==> 7 + 3 * x
--------------

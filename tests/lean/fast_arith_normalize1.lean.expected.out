>> x + y = x + y ==> 0 = 0
>> x + y = y + x ==> 0 = 0
>> 2 * x + y = y + 2 * x ==> 0 = 0
>> 2 * x + 3 * y = 3 * y + 2 * x ==> 0 = 0
--------------
>> 2 * x = x ==> x = 0
>> x = 2 * x ==> -1 * x = 0
>> 2 * x = 3 * x ==> -1 * x = 0
>> 3 * x = 2 * x ==> x = 0
--------------
>> 1 = x ==> 1 = x
>> x = y + x ==> 0 = y
>> x = x + y ==> 0 = y
>> 1 + x = x ==> 0 = -1
>> 0 = x + y ==> 0 = x + y
>> 1 = x + y ==> 1 = x + y
--------------
>> 0 = x + y ==> -1 * x + -1 * y = 0
>> 1 = x + y ==> -1 * x + -1 * y = -1
>> 0 = x + y + z ==> -1 * x + (-1 * y + -1 * z) = 0
>> -1 = -1 * x + -1 * y + -1 * z ==> x + (y + z) = 1
